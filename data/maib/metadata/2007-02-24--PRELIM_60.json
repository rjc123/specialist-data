  "title": "Completed PE Summary - Prudence", 
  "raw_html": "<div id=\"mainContentArea\" style=\"width: 545px;\"><div id=\"pageTitleContainer\">\r\n<p>\r\n</p>\r\n</div>\r\n<div id=\"pageTitleContainer\">\r\n<p>\r\n</p>\r\n</div>\r\n<div id=\"TextContainer\"><!--StartFragment-->\r\r\n<table border=\"1\" style=\"border-collapse:collapse;border:none;mso-border-alt:solid windowtext .5pt; mso-yfti-tbllook:480;mso-padding-alt:0cm 5.4pt 0cm 5.4pt;mso-border-insideh: .5pt solid windowtext;mso-border-insidev:.5pt solid windowtext\" class=\"MsoTableGrid\" cellspacing=\"0\" cellpadding=\"0\">\r\r\n    <tr style=\"mso-yfti-irow:0\">\r\r\n        <td valign=\"top\" width=\"284\" style=\"width:213.05pt;border:solid windowtext 1.0pt; mso-border-alt:solid windowtext .5pt;padding:0cm 5.4pt 0cm 5.4pt\">\r\r\n             <span style=\"mso-bidi-font-weight:normal\"><span style=\"font-family:Arial; font-size:10pt; \"><span style=\"font-size:10.0pt;font-family:Arial;font-weight:bold; mso-bidi-font-weight:normal\">Category:</span></span></span>\r\r\n        </td>\r\r\n        <td valign=\"top\" width=\"284\" style=\"width:213.05pt;border:solid windowtext 1.0pt; border-left:none;mso-border-left-alt:solid windowtext .5pt;mso-border-alt: solid windowtext .5pt;padding:0cm 5.4pt 0cm 5.4pt\">\r\r\n             <span style=\"font-family:Arial; font-size:10pt; \"><span style=\"font-size:10.0pt; font-family:Arial\">Merchant vessels</span></span>\r\r\n        </td>\r\r\n    </tr>\r\r\n    <tr style=\"mso-yfti-irow:1\">\r\r\n        <td valign=\"top\" width=\"284\" style=\"width:213.05pt;border:solid windowtext 1.0pt; border-top:none;mso-border-top-alt:solid windowtext .5pt;mso-border-alt:solid windowtext .5pt; padding:0cm 5.4pt 0cm 5.4pt\">\r\r\n             <span style=\"mso-bidi-font-weight:normal\"><span style=\"font-family:Arial; font-size:10pt; \"><span style=\"font-size:10.0pt;font-family:Arial;font-weight:bold; mso-bidi-font-weight:normal\">Registered Owner:</span></span></span>\r\r\n        </td>\r\r\n        <td valign=\"top\" width